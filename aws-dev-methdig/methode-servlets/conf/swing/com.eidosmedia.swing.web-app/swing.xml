<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE EOMVAR SYSTEM "file:../cfg/eomvar.dtd">
<WebClientConfiguration>
	<global>
		<logEnabled>false</logEnabled>
                <connectionId>&BACKEND_ENV1;</connectionId>
		<casEnabled>False</casEnabled>
                <analyticsEnabled>false</analyticsEnabled>
                <showBetaFeatures>false</showBetaFeatures>
		<eventServerUri>&LB_EOMJNC_URL;</eventServerUri>
		<defaultCorrelationName1>see_also</defaultCorrelationName1>
		<language>en</language>
		<loggerType>console</loggerType>
		<proxy>server</proxy>
		<debugEnabled>true</debugEnabled> <!--should be false in Prod-->
		<cobaltEnabled>false</cobaltEnabled>
		<primeServerEnabled>false</primeServerEnabled>
		<cacheEnabled>true</cacheEnabled>
		<automaticSaveTimer>5000</automaticSaveTimer>
		<defaultCorrelationName>see_also</defaultCorrelationName>
		<methodeApplicationServerUri>&LB_MRAS_CORE_URL;&LB_MRAS_CORE_CONTEXT;</methodeApplicationServerUri>
                <experimentalFeatures>
                        <advancedoembed>true</advancedoembed>
                        <cobaltfeatures>true</cobaltfeatures>
                        <shapechange>true</shapechange>
                        <linkpage>true</linkpage>
                </experimentalFeatures>
	</global>
        <!-- 
        <forceCSSReflow>true</forceCSSReflow>
        --> 
	<proxies>
        	<proxy>
                        <name>eddevServices</name>
                        <targetUri>&HTTP_PROTO;://&TOMCAT_CUSTOM_HOST;:&TOMCAT_CUSTOM_PORT;/eddevServices</targetUri>
                </proxy>
		<proxy>
                        <name>canonical</name>
                        <targetUri>&HTTP_PROTO;://&SERVER_TOMCAT_CORE;:&TOMCAT_CORE_PORT;/quickmetadata/blog/proxy.jsp</targetUri>
                </proxy>
		<proxy>
                        <name>canonical2</name>
                        <targetUri>&HTTP_PROTO;://&SERVER_TOMCAT_CORE;:&TOMCAT_CORE_PORT;/quickmetadata/blog/proxy2.jsp</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/discovery</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/core</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/core</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/moderation</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/moderation</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/cobaltpub</name>
                        <targetUri>&HTTP_PROTO;://&SERVER_COBALT_PUB;:&COBALT_PORT;/cobaltpub</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/directory</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/directory</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/abtesting</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/abtesting</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/discovery</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/discovery</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/comments</name>
                        <targetUri>&COBALT_CORE_PUBLIC_BASE_URL;/comments</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/web</name>
                        <targetUri>&COBALT_FRONTEND_PUBLIC_BASE_URL;</targetUri>
                </proxy>
		<proxy>
                        <name>cobalt/frontend</name>
                        <targetUri>&COBALT_FRONTEND_PUBLIC_BASE_URL;</targetUri>
                </proxy>
	</proxies>
</WebClientConfiguration>
